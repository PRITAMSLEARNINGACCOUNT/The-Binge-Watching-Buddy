{
  "version": 2,
  "builds": [
    {
      "src": "manage.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb" }
    },
    {
      "src": "static/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "tmp/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/tmp/(.*)",
      "dest": "/tmp/$1"
    },
    {
      "src": "/(.*)",
      "dest": "manage.py"
    }
  ]
}
